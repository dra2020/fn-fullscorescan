{
  "name": "@dra2020/fn-fullscorescan",
  "version": "1.0.0",
  "description": "AWS Lambda function for ensuring all maps are scored.",
  "main": "dist/fn-fullscorescan.js",
  "types": "./dist/fn-fullscorescan.d.ts",
  "scripts": {
    "test": "node testdist/test.js",
    "debug": "node --inspect --inspect-brk testdist/test.js",
    "build": "webpack",
    "prezip": "rm -f deploy.zip",
    "fullzip": "npm run prezip && zip -q -r deploy.zip package.json package-lock.json dist config.js index.js LICENSE README.md",
    "awsdeploy": "aws lambda update-function-code --function fullScoreScan --zip-file fileb://deploy.zip",
    "fulldeploy": "npm run fullzip && npm run awsdeploy",
    "invoke": "aws lambda invoke --function-name fullScoreScan out --log-type Tail",
    "webpack": "node_modules/.bin/webpack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dra2020/fn-fullscorescan.git"
  },
  "keywords": [
    "Typescript"
  ],
  "author": "Terry Crowley",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dra2020/fn-fullscorescan/issues"
  },
  "homepage": "https://github.com/dra2020/fn-fullscorescan#readme",
  "devDependencies": {
    "@types/node": "^12.19.9",
    "@types/object-hash": "^1.3.4",
    "json-loader": "^0.5.7",
    "source-map-loader": "^1.1.3",
    "ts-loader": "^8.0.12",
    "tsify": "^5.0.2",
    "typescript": "^4.1.3",
    "webpack": "^5.10.3",
    "webpack-cli": "^4.2.0"
  },
  "dependencies": {
    "@dra2020/context": "^1.0.15",
    "@dra2020/dbabstract": "^2.0.1",
    "@dra2020/dbdynamo": "^2.0.1",
    "@dra2020/dra-types": "^1.6.6",
    "@dra2020/fsm": "^1.3.3",
    "@dra2020/lambda": "^1.1.1",
    "@dra2020/logabstract": "^1.0.16",
    "@dra2020/logserver": "^2.0.1",
    "@dra2020/ot-editutil": "^2.1.8",
    "@dra2020/ot-js": "^2.0.50",
    "@dra2020/poly": "^1.7.26",
    "@dra2020/storage": "^2.0.2",
    "@dra2020/storages3": "^2.0.1",
    "@dra2020/util": "^1.1.4"
  }
}
